node {
  stage 'Testing Jenkins and Github Integration'
  sh '/home/etg-user/Downloads/apache-jmeter-5.3/bin/./jmeter -n -t /home/etg-user/Downloads/apache-jmeter-5.3/bin/Demo.jmx -l test.jtl'

import base64
from github import Github
from github import InputGitTreeElement

user = "knani1222"
password = "Github@123456"
g = Github(user,password)
repo = g.get_user().get_repo('git-test')
file_list = [
    '/var/lib/jenkins/workspace/tutorial\test.jtl',
   
]

file_names = [
    'test.jtl',

]
commit_message = 'jenkins Integration'
master_ref = repo.get_git_ref('heads/master')
master_sha = master_ref.object.sha
base_tree = repo.get_git_tree(master_sha)
element_list = list()
for i, entry in enumerate(file_list):
    with open(entry) as input_file:
        data = input_file.read()
    if entry.endswith('.jtl'):
        data = base64.b64encode(data)
    element = InputGitTreeElement(file_names[i], '100644', 'blob', data)
    element_list.append(element)
tree = repo.create_git_tree(element_list, base_tree)
parent = repo.get_git_commit(master_sha)
commit = repo.create_git_commit(commit_message, tree, [parent])
master_ref.edit(commit.sha)


}
